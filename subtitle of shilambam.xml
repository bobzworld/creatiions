          <anchoredRegion t="0:04:29.20" d="0" />
          <anchoredRegion t="0:04:33.994" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_53" author="Subtitle Edit" type="text" style="speech">
      <TEXT>masculine. Very aggressive,&lt;br&gt;literally like no space for</TEXT>
