          <anchoredRegion t="0:02:20.515" d="0" />
          <anchoredRegion t="0:02:23.795" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_35" author="Subtitle Edit" type="text" style="speech">
      <TEXT> [Bodhidharmam] learnt the techniques&lt;br&gt;of Silambam and</TEXT>
      <segment>
        <movingRegion type="anchored">
          <anchoredRegion t="0:02:24.925" d="0" />
          <anchoredRegion t="0:02:27.860" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_36" author="Subtitle Edit" type="text" style="speech">
      <TEXT> took it all across Asia</TEXT>
      <segment>
        <movingRegion type="anchored">
          <anchoredRegion t="0:02:27.905" d="0" />
          <anchoredRegion t="0:02:29.775" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_37" author="Subtitle Edit" type="text" style="speech">
