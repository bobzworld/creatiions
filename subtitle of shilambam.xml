        <movingRegion type="anchored">
          <anchoredRegion t="0:04:34.194" d="0" />
          <anchoredRegion t="0:04:37.532" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_54" author="Subtitle Edit" type="text" style="speech">
