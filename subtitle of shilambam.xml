        <movingRegion type="anchored">
          <anchoredRegion t="0:02:46.997" d="0" />
          <anchoredRegion t="0:02:48.895" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_41" author="Subtitle Edit" type="text" style="speech">
      <TEXT>Even if it is just a basic rotation</TEXT>
