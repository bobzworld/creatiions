      <segment>
        <movingRegion type="anchored">
          <anchoredRegion t="0:01:49.411" d="0" />
          <anchoredRegion t="0:01:51.890" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_27" author="Subtitle Edit" type="text" style="speech">
      <TEXT>They would pick up a stick&lt;br&gt;that they would find to help</TEXT>
