        <movingRegion type="anchored">
          <anchoredRegion t="0:02:48.927" d="0" />
          <anchoredRegion t="0:02:50.779" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_42" author="Subtitle Edit" type="text" style="speech">
      <TEXT>There is so much of strength and control&lt;br&gt;that is goes into</TEXT>
      <segment>
        <movingRegion type="anchored">
          <anchoredRegion t="0:02:51.367" d="0" />
          <anchoredRegion t="0:02:54.734" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_43" author="Subtitle Edit" type="text" style="speech">
      <TEXT>every single movement</TEXT>
      <segment>
        <movingRegion type="anchored">
          <anchoredRegion t="0:02:55.194" d="0" />
          <anchoredRegion t="0:02:56.730" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_44" author="Subtitle Edit" type="text" style="speech">
      <TEXT>When both your body and your mind is&lt;br&gt;focused on one particular activity.</TEXT>
      <segment>
        <movingRegion type="anchored">
          <anchoredRegion t="0:03:01.378" d="0" />
          <anchoredRegion t="0:03:04.152" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_45" author="Subtitle Edit" type="text" style="speech">
      <TEXT>It's almost like meditation</TEXT>
