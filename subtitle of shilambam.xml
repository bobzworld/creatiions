        <movingRegion type="anchored">
          <anchoredRegion t="0:01:47.371" d="0" />
          <anchoredRegion t="0:01:49.340" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_26" author="Subtitle Edit" type="text" style="speech">
