    <annotation id="annotation_24" author="Subtitle Edit" type="text" style="speech">
      <TEXT>Her simple sight has the&lt;br&gt;spirit of all three</TEXT>
      <segment>
        <movingRegion type="anchored">
          <anchoredRegion t="0:01:42.914" d="0" />
          <anchoredRegion t="0:01:45.213" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_25" author="Subtitle Edit" type="text" style="speech">
      <TEXT> Thousands of years back&lt;br&gt;when people were walking</TEXT>
