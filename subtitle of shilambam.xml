        <movingRegion type="anchored">
          <anchoredRegion t="0:02:02.771" d="0" />
          <anchoredRegion t="0:02:05.70" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_30" author="Subtitle Edit" type="text" style="speech">
      <TEXT>or they have to defend themselves&lt;br&gt;from another fellow human being</TEXT>
      <segment>
        <movingRegion type="anchored">
          <anchoredRegion t="0:02:05.269" d="0" />
          <anchoredRegion t="0:02:08.639" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_31" author="Subtitle Edit" type="text" style="speech">
