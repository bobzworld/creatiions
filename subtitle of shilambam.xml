          <anchoredRegion t="0:03:27.643" d="0" />
          <anchoredRegion t="0:03:31.806" d="0" />
        </movingRegion>
      </segment>
    </annotation>
    <annotation id="annotation_49" author="Subtitle Edit" type="text" style="speech">
